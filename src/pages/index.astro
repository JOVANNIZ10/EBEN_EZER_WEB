---
import "../styles/global.css";
import MainLayout from "../layouts/MainLayout.astro";
import Header from "../components/Header.astro";
import Hero from "../components/Hero.astro";
import Nosotros from "../components/Nosotros.astro";
import Galeria from "../components/Galeria.astro";
import Footer from "../components/Footer.astro";
---

<MainLayout>
  <Header slot="header" />
  <Hero slot="hero"
    image="/images/foto.jpg"
    title="Eben Ezer"
    subtitle="Un grupo perteneciente a la Iglesia Pentecostal Elim Beasain"
  />
  <Nosotros slot="nosotros"/>
  <Galeria slot="galeria"/>
  <Footer slot="footer">
</MainLayout>

